{
  "name": "react-any-shape-form",
  "author": "Eugene Trofimov",
  "license": "ISC",
  "version": "2.0.3",
  "description": "Small, type-friendly and flexible react form component.",
  "keywords": [
    "form",
    "typescript",
    "validation",
    "react",
    "antd",
    "ant",
    "styleless",
    "replacement"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deadrime/react-any-shape-form.git"
  },
  "homepage": "https://react-any-shape-form.vercel.app/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "module": "dist/bundle.es.js",
  "exports": {
    ".": {
      "import": "./dist/bundle.es.js",
      "types": "./dist/src/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*",
      "require": "./dist/*",
      "types": "./dist/src/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "types": "dist/src/index.d.ts",
  "dependencies": {
    "react-use": "^17.4.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-storysource": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@storybook/addon-docs": "^8.0.9",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-onboarding": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-vite": "^8.0.9",
    "@types/cross-spawn": "^6.0.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.0.9",
    "terser": "^5.26.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.2"
  }
}
